package telusko.Threads;

class Hi{
	
	public void show() {
		
		for(int i = 0; i < 5; i++) {
			
			System.out.println("Hi"); 
		}
	}
}

class Hello{
	
	public void show() {
		
		for(int i = 0; i < 5; i++) {
			
			System.out.println("Hello"); 
		}
	}
}

