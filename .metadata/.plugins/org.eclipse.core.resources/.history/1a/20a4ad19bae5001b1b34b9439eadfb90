package telusko.my.own.exception;

public class ExceptionExample {

	public static void main(String[] args) {
		
		int a = 9;
		int b = 8;
		
		try {
			System.out.println(a/b);
			if(b == 0)
				throw new MyException("This is not possible");
					
		}catch(MyException e) {
			System.out.println(e.getMessage()); 
		}

	}

}
